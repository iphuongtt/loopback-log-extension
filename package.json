{
    "name": "loopback-log-extension",
    "version": "1.0.22",
    "description": "loopback log extension",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "lb-tsc",
        "build:watch": "lb-tsc --watch",
        "clean": "lb-clean dist *.tsbuildinfo",
        "lint": "npm run prettier:check && npm run eslint",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
        "prettier:check": "npm run prettier:cli -- -l",
        "prettier:fix": "npm run prettier:cli -- --write",
        "eslint": "lb-eslint --report-unused-disable-directives .",
        "eslint:fix": "npm run eslint -- --fix",
        "pretest": "npm run clean && npm run build",
        "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
        "posttest": "npm run lint",
        "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git"
    },
    "author": "iphuongtt",
    "license": "",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "dependencies": {
        "@loopback/boot": "^8.0.8",
        "@loopback/context": "^8.0.7",
        "@loopback/core": "^7.0.7",
        "@loopback/repository": "^8.0.7",
        "@loopback/rest": "^15.0.8",
        "axios": "^1.13.2",
        "ip": "^2.0.1",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@loopback/build": "^12.0.7",
        "@loopback/eslint-config": "^16.0.1",
        "@loopback/testlab": "^8.0.7",
        "@types/ip": "^1.1.3",
        "@types/node": "^25.0.3",
        "@typescript-eslint/eslint-plugin": "^8.50.1",
        "@typescript-eslint/parser": "^8.50.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-eslint-plugin": "^7.2.0",
        "eslint-plugin-mocha": "^11.2.0",
        "source-map-support": "^0.5.21",
        "typescript": "~5.9.3"
    }
}
